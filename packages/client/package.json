{
  "name": "@shadle/client",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm run prebuild && vite build",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist/ src/auto-imports.d.ts src/components.d.ts vite.config.ts.timestamp-*",
    "clean:all": "pnpm run clean && git clean -fdX",
    "dev": "pnpm run prebuild && vite dev --port 3333 --clearScreen false",
    "prebuild": "tsx scripts/prebuild.ts",
    "preview": "vite preview --port 3000",
    "test": "NODE_ENV=test vitest run"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.14",
    "@shadle/types": "workspace:*",
    "nprogress": "^0.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "unocss": "^66.5.5",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@unocss/eslint-config": "^66.5.5",
    "@unocss/eslint-plugin": "^66.5.5",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^5.1.0",
    "caniuse-lite": "^1.0.30001754",
    "jsdom": "^27.1.0",
    "sharp": "^0.34.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
