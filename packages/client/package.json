{
  "name": "@shadle/client",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsx scripts/prebuild.ts && vite build && tsx scripts/postbuild.ts",
    "check": "vue-tsc --noEmit",
    "clean": "rm -rf dist/ src/auto-imports.d.ts src/components.d.ts vite.config.ts.timestamp-*",
    "clean:all": "pnpm run clean && git clean -fdX",
    "dev": "tsx scripts/prebuild.ts && COMMIT_HASH=$(git rev-parse HEAD) NODE_ENV=dev vite dev --port 3333 --host --clearScreen false",
    "test": "NODE_ENV=test vitest run"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.14",
    "@shadle/types": "workspace:*",
    "@unhead/vue": "^2.0.19",
    "@unocss/reset": "^66.5.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue-macros/volar": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "unocss": "^66.5.6",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-macros": "^2.14.5",
    "unplugin-vue-markdown": "^29.2.0",
    "unplugin-vue-router": "^0.17.0",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vue": "^3.5.24",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.6.3",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.3",
    "@vite-pwa/assets-generator": "^1.0.2",
    "caniuse-lite": "^1.0.30001755",
    "jsdom": "^27.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10",
    "vue-tsc": "^3.1.4"
  }
}
