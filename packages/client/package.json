{
  "name": "@shadle/client",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=prod vite build",
    "check": "vue-tsc --noEmit",
    "clean": "rm -rf dist/ src/auto-imports.d.ts src/components.d.ts vite.config.ts.timestamp-*",
    "clean:all": "pnpm run clean && git clean -fdX",
    "dev": "COMMIT_HASH=$(git rev-parse HEAD) NODE_ENV=dev vite dev --port 3333 --host --clearScreen false",
    "preview": "COMMIT_HASH=$(git rev-parse HEAD) pnpm run build && vite preview --port 3000 --host",
    "test": "NODE_ENV=test vitest run"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.14",
    "@shadle/types": "workspace:*",
    "@unhead/vue": "^2.0.19",
    "@unocss/reset": "^66.5.9",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue-macros/volar": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vuepic/vue-datepicker": "^12.0.5",
    "@vueuse/core": "^14.1.0",
    "canvas-confetti": "^1.9.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "unocss": "^66.5.9",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-macros": "^2.14.5",
    "unplugin-vue-markdown": "^29.2.0",
    "unplugin-vue-router": "^0.17.2",
    "vite": "^7.2.4",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/nprogress": "^0.2.3",
    "caniuse-lite": "^1.0.30001757",
    "jsdom": "^27.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.5"
  }
}
